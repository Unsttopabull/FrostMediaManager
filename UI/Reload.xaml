<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:podatki="clr-namespace:ObdelajProdatke;assembly=ObdelajProdatke"
    x:Class="WPF_Jukebox.Reload"
    x:Name="Window"
    Title="Reload"
    Width="217.6" Height="151.467">

    <Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="59*" />
            <RowDefinition Height="55*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="115*" />
            <ColumnDefinition Width="110*" />
        </Grid.ColumnDefinitions>
        <Label x:Name="Label1" Content="Tip sistema:" Margin="10,5,9.2,30.4" />
        <ComboBox x:Name="ComboBox" Margin="10,34,8.6,3.4" Grid.ColumnSpan="2" SelectedIndex="0"
                  SelectedItem="{Binding SistemKnjiznice, ElementName=Window, FallbackValue=nič, Mode=OneTime, TargetNullValue=nič}" ItemsSource="{Binding ElementName=Window, Path=SistemNames}"/>
        <Button Content="Button" Margin="10,9.6,9.2,25.6" ClipToBounds="True" Grid.Row="1" />
        <Button Content="Button" Margin="10.8,9.6,8.6,25.6" Grid.Column="1" ClipToBounds="True" Grid.Row="1" />
    </Grid>
</Window>